{
    "version": 2,
    "name": "ting-chatApp-backend",
    "builds": [{ "src": "index.js", "use": "@vercel/node" }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.js",
            "headers": {
                "Access-Control-Allow-Origin": "http://ting-chat-app-backend.vercel.app",
                "Access-Control-Allow-Methods": "GET, POST"
            }
        }
    ]
}
